<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= form_tag users_path, :method => 'get' do %>
  <p style= "width: 32.5%">
    <%= text_field_tag :search, params[:search], placeholder: "Search Users", class: "text_tags" %>
    <%= submit_tag "Search", class: 'btn btn-small btn-inverse', :name => nil %>
  </p>

<% end %>

<%= will_paginate %>
<ul class="users">
  <%= render @users %> 
</ul>
<%= will_paginate %>